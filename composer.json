{
  "name": "fixer112/laravel-sanitizer",
  "description": "Laravel middleware to sanitize inputs and block malicious bots and codes",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Fixer112\\Sanitizer\\": "src/"
    }
  },
  "require": {
    "php": ">=8.0",
    "laravel/framework": ">=10.0.0"
  },
  
  "scripts": {
    "rector": "vendor/bin/rector",
    "pint": "vendor/bin/pint",
    "pint:file": "vendor/bin/pint --",
    "fix": "composer rector && composer pint"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Fixer112\\Sanitizer\\SanitizerServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require-dev": {
    "laravel/pint": "^1.24",
    "larastan/larastan": "^3.5",
    "rector/rector": "^2.1",
    "phpunit/phpunit": ">=4.8",
    "laravel/framework": ">=10"
  }
}
